{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://skillvouch-hexart-2026.onrender.com/api/$1"
    }
  ],
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist"
}
