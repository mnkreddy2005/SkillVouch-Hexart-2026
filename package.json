{
  "name": "skillvouch-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:frontend": "cd Frontend && vite",
    "dev:backend": "cd Backend && node server.js",
    "build": "cd Frontend && npm install && npx vite build",
    "build:frontend": "cd Frontend && npm install && npx vite build",
    "build:backend": "cd Backend && npm install",
    "build:render": "npm run build:frontend && npm run build:backend",
    "preview": "cd Frontend && vite preview",
    "start": "cd Backend && node server.js",
    "install:all": "npm install && cd Frontend && npm install && cd ../Backend && npm install"
  },
  "dependencies": {
    "@langchain/core": "^1.1.8",
    "@langchain/mistralai": "^1.0.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "firebase": "^12.6.0",
    "firebase-tools": "^14.27.0",
    "lucide-react": "^0.556.0",
    "mysql2": "^3.15.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "recharts": "^3.5.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^22.19.3",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.23",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
